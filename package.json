{
  "name": "express-typescript",
  "version": "0.0.1",
  "description": "Express Typescript",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon dist/server.js",
    "start": "ts-node dist/server.js",
    "build": "tsc",
    "watch-ts": "tsc -w",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "test": "",
    "typeorm": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Phoique/express-typescript.git"
  },
  "author": "Enes Kahraman <official.phoique@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Phoique/express-typescript/issues"
  },
  "homepage": "https://github.com/Phoique/express-typescript#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^4.0.2",
    "express-session": "^1.17.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "@types/express-handlebars": "3.1.0",
    "@types/express-session": "1.17.0",
    "@types/node": "13.13.5",
    "@types/shelljs": "0.8.7",
    "@typescript-eslint/eslint-plugin": "2.30.0",
    "@typescript-eslint/parser": "2.30.0",
    "eslint": "6.8.0",
    "nodemon": "2.0.3",
    "shelljs": "0.8.4",
    "ts-node": "8.9.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.8.3"
  }
}
